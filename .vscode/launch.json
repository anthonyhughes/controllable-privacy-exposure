    "version": "0.2.0",
    "configurations": [
        {
            "name": "Legal Psuedo",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${cwd}"
            },
            "args": [
                "--task", "legal_court",
                "--plib", "spacy"
            ]
        },
        {
            "name": "Ollama Pipeline",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${cwd}"
            },
            "args": [
                "--task", "legal_court",
                "--model", "llama3.1:70b",
            ]
        },
        {
            "name": "OpenAI Pipeline",
            "type": "debugpy",
            "request": "launch",
            "program": "pipelines/openai_pipeline.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${cwd}"
            },
            "args": [
                "--task", "legal_court",
                "--model", "gpt-4o-mini",
            ],
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Claude Sonnet Pipeline",
            "type": "debugpy",
            "request": "launch",
            "program": "pipelines/claude_pipeline.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${cwd}"
            },
            "args": [
                "--task", "legal_court",
                "--model", "claude-3-5-sonnet-20240620",
            ],
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Eval Pipeline",
            "type": "debugpy",
            "request": "launch",
            "program": "pipelines/eval_pipeline.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${cwd}"
            },
            "args": [
                "--tasks", "brief_hospital_course",
                "--model", "llama3.1:70b",
            ],
        }
    ]
}